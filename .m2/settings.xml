<settings>
    <servers>
        <server>
            <id>ossrh</id>
            <!-- your-jira-id -->
            <username>{}</username>
            <!-- your-jira-pwd -->
            <password>{}</password>
        </server>
    </servers>
    <profiles>
        <profile>
            <id>ossrh</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <gpg.executable>gpg</gpg.executable>
                <!-- the_pass_phrase -->
                <gpg.passphrase>{}</gpg.passphrase>
            </properties>
        </profile>
    </profiles>
</settings>
